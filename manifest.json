{
  "manifest_version": 3,
  "name": "Floating Dock",
  "version": "1.1",
  "description": "A macOS-style floating dock for quick navigation.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
